        </activity>
        <activity
            android:name="com.example.smsdemo.Wdedd"
            android:label="约拍-订单" >
        </activity>
        <activity
            android:name="com.example.smsdemo.Onesix"
            android:label="约拍-详情" >
        </activity>
        <activity
            android:name="com.example.smsdemo.Shijian"
            android:label="约拍-设置时间" >
        </activity>
        <activity
            android:name="com.example.smsdemo.Yuyue"
            android:label="约拍-预约" >
